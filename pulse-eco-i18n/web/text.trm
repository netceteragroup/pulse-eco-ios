	<text key="measurements.humidity.orangeBand.shortGrade">
		<context>measurements</context>
		<value lang="de" status="translated">Trockene Luft.</value>
		<value lang="en" status="translated">Dry air.</value>
		<value lang="mk" status="translated">Сув воздух.</value>
		<value lang="ro" status="translated">Aer uscat.</value>
